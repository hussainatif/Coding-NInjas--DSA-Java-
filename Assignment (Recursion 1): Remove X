public class solution {

	// Return the changed string
	public static String removeX(String str){
		// Write your code here
        if (str.length() == 0)
    {
        return "";
    }
         if (str.charAt(0) == 'x')
    {
        return removeX(str.substring(1));
    }
        return str.charAt(0) + removeX(str.substring(1));

	}
}
